{
  "expo": {
    "name": "glamTimeApp",
    "slug": "glamTimeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.glamTime",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.glamTime.glamTimeApp",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.${ios.bundleIdentifier}.onesignal"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.glamTime.glamTimeApp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAsW7SWTUZDXYgQpn67ifBBY3c-vghx2XQ"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "onesignal-expo-plugin",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "textColor": "#000000",
              "colorAccent": {
                "light": "#000000",
                "dark": "#FFFFFF"
              },
              "textColorPrimary": {
                "light": "#FFFFFF",
                "dark": "#000000"
              }
            }
          }
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./assets/images/default.png"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supabaseUrl": "",
      "supabaseAnonKey": "",
      "googleMapsApiKey": "",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09907518-f6ae-463a-b3f9-2390f4f43e31"
      }
    }
  }
}

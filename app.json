{
  "expo": {
    "name": "glamTimeApp",
    "slug": "glamTimeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.glamTime",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.glamTime.glamTimeApp",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.${ios.bundleIdentifier}.onesignal"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.glamTime.glamTimeApp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAsW7SWTUZDXYgQpn67ifBBY3c-vghx2XQ"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supabaseUrl": "",
      "supabaseAnonKey": "",
      "googleMapsApiKey": "",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09907518-f6ae-463a-b3f9-2390f4f43e31"
      }
    }
  }
}
